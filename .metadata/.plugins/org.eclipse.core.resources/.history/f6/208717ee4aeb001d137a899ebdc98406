package com.Masai;
import java.util.ArrayList;
import java.util.List;
public class Customer {
	String cName;
	int cNumOfStocks;
	String cUserId;
	String cPassword;
	int customerId;
	List<Stock> cS;
	public Customer() {
		
	}
	public Customer(String cName,String cUserId,String cPassword)
	{
		this.cName = cName;
		this.cUserId = cUserId;
		this.cPassword = cPassword;
	}
}
